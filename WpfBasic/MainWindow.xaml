<Window x:Class="WpfBasic.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfBasic"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="1*"/>
            <RowDefinition Height="5*"/>
        </Grid.RowDefinitions>
        <StackPanel HorizontalAlignment="Center" Orientation="Horizontal">
            <Button Name="Back" FontSize="20" Click="Back_Click">&lt;</Button>
            <Button Name="Front" FontSize="20" Click="Front_Click">&gt;</Button>
        </StackPanel>
        <Frame Name="frm" Grid.Row="1" Source="/View/Page1.xaml" NavigationUIVisibility="Hidden"/>


    </Grid>
</Window>
